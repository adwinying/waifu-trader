{
  "private": true,
  "name": "waifu-trader",
  "description": "",
  "license": "",
  "scripts": {
    "build": "run-s -l build:css build:remix",
    "build:remix": "remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "env-cmd npm run dev:base",
    "dev:base": "run-p -l dev:css dev:remix",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "lint": "run-s -l lint:*",
    "lint:es": "eslint \"{app,prisma,test,cypress}/**/*.{js,jsx,ts,tsx}\"",
    "lint:ts": "tsc --noEmit --project tsconfig.json",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "run-s cypress:dbseed cypress:run",
    "cypress:dbseed": "rm -f ./prisma/test.db ./prisma/test.db.bak && env-cmd -f .env.test prisma migrate deploy && cp ./prisma/test.db ./prisma/test.db.bak",
    "cypress:server": "env-cmd -f .env.test npm run dev:base",
    "cypress:open": "start-server-and-test cypress:server 3002 'cypress open'",
    "cypress:run": "start-server-and-test cypress:server 3002 'cypress run'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "daisyui": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.5.1",
    "vitest": "^0.14.2",
    "vitest-mock-extended": "^0.1.4",
    "zod": "^3.11.6",
    "zod-form-data": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "c8": "^7.11.3",
    "cypress": "^9.2.0",
    "env-cmd": "^10.1.0",
    "esbuild-register": "^3.3.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "prisma": "^3.8.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.8",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "~16"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed/index.ts"
  },
  "sideEffects": false
}
